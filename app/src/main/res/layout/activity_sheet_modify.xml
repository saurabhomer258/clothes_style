            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="6">

            <com.example.saurabhomer.style2.customView.NextButton
                android:id="@+id/btn_next"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
