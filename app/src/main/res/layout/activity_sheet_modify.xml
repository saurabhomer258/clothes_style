            <TextView
                android:id="@+id/time1"
                android:text="8.15 - 9.05"
                android:layout_height="wrap_content"
                />

            <TextView
                android:id="@+id/lap1"
                android:text="1"
                android:layout_height="wrap_content"
                />
            <EditText
                android:id="@+id/output1"
                android:inputType="number"
                android:text="0"
                android:layout_height="wrap_content"
                />

            <EditText
                android:id="@+id/target1"
                android:inputType="number"
                android:text="0"
                android:layout_height="wrap_content"
                />
